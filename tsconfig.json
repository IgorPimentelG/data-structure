{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es2016",
      "dom"
    ],
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "downlevelIteration": true
  }
}